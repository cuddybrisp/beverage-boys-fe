{{#each beverage as |beverage|}}
  <section class="beverage-index" data-id="{{bevarage._id}}">
    <h6>Beverage:</h6>
    <h3>
      {{limit beverage.name 40}}
    </h3>
    <h6>Ounces:</h6>
    <p>
        {{beverage.ounces}}
    </p>
    <h6>ID:</h6>
    <p>
      {{beverage._id}}
    </p>
{{> bevdelete id=beverage._id }}
  </section>
{{/each}}
